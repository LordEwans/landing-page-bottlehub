    "@sveltejs/kit": "^1.0.0-next.587",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "daisyui": "latest",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.31",
